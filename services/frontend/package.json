  "devDependencies": {
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.1.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-scripts": "3.1.1",
