{
  "name": "eosrate",
  "author": {
    "name": "EOS Costa Rica",
    "url": "https://eosio.cr/"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged && lint-staged",
    "lint": "eslint .",
    "start": "env-cmd -f .env react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "prod": "serve -l 9000 --single ./build",
    "build": "env-cmd -f .env react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "env-cmd -f .env react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "now": "now --public --npm && now alias",
    "now-start": "serve --single ./build"
  },
  "dependencies": {
    "@eoscostarica/eoscr-components": "^1.0.2",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@reach/router": "^1.3.3",
    "@rematch/core": "^1.4.0",
    "@rematch/persist": "^1.1.6",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.3",
    "babel-loader": "~8.0.6",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "env-cmd": "^10.1.0",
    "eosjs": "^20.0.3",
    "filter-objects": "^2.1.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "i18n-iso-countries": "^5.0.0",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.uniq": "^4.5.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.1",
    "random-color-rgb": "^1.1.1",
    "rc-slider": "^9.2.2",
    "react": "^16.13.0",
    "react-apollo": "^3.1.3",
    "react-autosuggest": "^9.4.3",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "16.13.0",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-highlighter": "^0.4.3",
    "react-i18next": "^11.3.3",
    "react-placeholder": "^4.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "serve": "^11.3.0",
    "subscriptions-transport-ws": "^0.9.16",
    "text-encoding": "^0.7.0",
    "ual-anchor": "^0.4.1",
    "ual-ledger": "^0.2.0",
    "ual-lynx": "^0.3.0",
    "ual-meetone": "^0.1.1",
    "ual-reactjs-renderer": "^0.2.0",
    "ual-scatter": "^0.2.0",
    "ual-token-pocket": "^0.2.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-scripts": "3.4.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
